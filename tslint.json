{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "class-name": true
    },
    "rules": {
        "no-any": true,
        "no-var": true,
        "indent": [true, "spaces", 4],
        "quotemark": [true, "single", "jsx-double"],
        "arrow-parens": true
    },
    "rulesDirectory": []
}